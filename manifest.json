{
  "manifest_version": 2,

  "name": "Webx",
  "description": "Extension for Webx web extraction.",
  "version": "0.0.1",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Webx"
  },
  "permissions": [
    "identity",
    "https://www.googleapis.com/",
    "https://accounts.google.com/"
  ]
}

